package com.example.demo.exception;

public enum ErrorEnums {
    USERNOTENOTFOUND("the user doesnÂ´t have that note"),
    NOTEIDNOTFOUND("Incorrect note id"),
    USERIDNOTFOUND("Incorrect user id");

